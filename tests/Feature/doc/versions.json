{
    "type": "object",
    "required": ["data", "version"],
    "additionalProperties": false,
    "properties": {
        "data": {
            "type": "object",
            "required": ["current", "dev", "stable", "deprecated"],
            "additionalProperties": false,
            "properties": {
                "current": {
                    "$ref": "#/$defs/version"
                },
                "dev": {
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/version"
                    }
                },
                "stable": {
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/version"
                    }
                },
                "deprecated": {
                    "type": "array",
                    "items": {
                        "$ref": "#/$defs/version"
                    }
                }
            }
        },
        "version": {
            "type": "string",
            "pattern": "^1\\.0$"
        }
    },
    "$defs": {
        "version": {
            "type": "object",
            "required": ["version"],
            "additionalProperties": false,
            "properties": {
                "version": {
                    "type": "string",
                    "minLength": 1
                }
            }
        }
    }
}
