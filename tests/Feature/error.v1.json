{
    "type": "object",
    "required": ["data", "version", "error"],
    "additionalProperties": false,
    "properties": {
        "version": {
            "type": "string",
            "pattern": "^1\\.0$"
        },
        "error": {
            "type": "string",
            "minLength": 1
        },
        "data": {
            "oneOf": [
                {"type": "null"},
                {"type": "object"}
            ]
        }
    }
}
